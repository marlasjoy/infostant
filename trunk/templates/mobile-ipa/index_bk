       {
         var catname = $(this).attr('id');  
       }
       $('#maplink').attr('href','mapcategory.html?category='+catname);
       $('#cattitle').html(ucfirst(catname));
                    $('#categoryResult').html('');
                          $.ajax({
                                                      data: {catname: catname},                                                            
                                                      url: webdir+'/ajax/getallcatbyname2',
                                                      dataType: "jsonp",
                                                      jsonp: 'callback',
                                                      jsonpCallback: 'jsonpCallback', 
                                                      crossDomain:true,
                                                      xhrFields: {
                                                      withCredentials: true
                                                      },
                                                       success: function(myObject){
                                               
                                                      }
                                                    });
       
       });
        if(getUrlVars2()['category'])
       {
